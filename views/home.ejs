<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <title>Footballkik - Home</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css"
        integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">

    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">
    <link rel="stylesheet" href="/css/style.css">
</head>

<body>
    <%- include('partials/navbar.ejs') -%>
    <div class="col-md-12">
        <form action="/results" method="post">
            <div class="form-group ad-30" style="margin-top:8px;">
                <div class="input-group search-style">
                    <input class="form-control new-search-bar" name="country" id="search"
                        placeholder="Enter Your Club Name" type="text">
                    <span class="input-group-btn">
                        <button class="btn input-btn" type="" id="search-form">Search</button>
                    </span>
                </div>
            </div>
        </form>
    </div>

    <div class="col-md-12">
        <input type="hidden" id="nameUser" value="Suraksha">
        <input type="hidden" id="nameImage" value="image">
        <div class="">
            <div class="row">
                <div class="col-md-3">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="filter-container">
                                <div class="card">
                                    <form action="/results" method="post">
                                        <div class="content">
                                            <p class="filter">
                                                FILTER
                                            </p>
                                            <hr />
                                            <select class="form-control" name="country" id="country">
                                                <option value="">Filter By Country</option>
                                                <%  _.forEach(country,function(value) {%>
                                                <option value="<%= value._id %>"><%= value._id %></option>
                                                <%}) %>
                                                <option value="= value._id">

                                                </option>

                                            </select>
                                            <div class="clearfix"></div>
                                            <button class="btn add-apply">Apply</button>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-9">
                    <div>
                        <% _.forEach(data, function(val){ %>
                        <div class="category-list">
                            <div class="row">
                                <% _.forEach(val, function(val2){ %>
                                <form action="/home" method="post" id="favorite">
                                    <a style="text-decoration:none" href="/group/<%= val2.name %>">
                                        <div class="item col-md-4">
                                            <div class="thumbnail">
                                                <img src="https://tienupuserimage.s3.amazonaws.com/<%= val2.image %>"
                                                    alt="">
                                                <div class="caption">
                                                    <h4>
                                                        <%= val2.name %>
                                                    </h4>
                                                    <div class="row">
                                                        <div class="col-xs-12 col-md-6">
                                                            <p>
                                                                <%= val2.country %>
                                                            </p>
                                                        </div>
                                                        <div class="col-xs-12 col-md-6">
                                                            <input type="hidden" name="id" id="id" value="= val2._id">
                                                            <input type="hidden" name="clubName" id="clubName"
                                                                value="= val2.name ">
                                                            <button class="btn add-fav">
                                                                Add To Favorite
                                                            </button>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </a>
                                    <% }) %> </form>
                            </div>

                        </div>
                        <% }) %>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"
        integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa"
        crossorigin="anonymous"></script>


</body>

</html>